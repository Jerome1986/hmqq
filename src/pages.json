{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/ai/ai",
      "style": {
        "navigationBarTitleText": "算鸟AI",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/find/find",
      "style": {
        "navigationBarTitleText": "发现",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/groupDetail/groupDetail",
      "style": {
        "navigationBarTitleText": "详情",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/discoveryDetail/discoveryDetail",
      "style": {
        "navigationBarTitleText": "种草详情",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/searchDetail/searchDetail",
      "style": {
        "navigationBarTitleText": "搜索",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/dialogue/dialogue",
      "style": {
        "navigationBarTitleText": "对话",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/shopList/shopList",
      "style": {
        "navigationBarTitleText": "适用门店",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false
      }
    }
  ],
  "subPackages": [
    {
      "root": "subPackages",
      "pages": [
        {
          "path": "setUserInfo/setUserInfo",
          "style": {
            "navigationBarTitleText": "个人信息",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "collect/collect",
          "style": {
            "navigationBarTitleText": "我的收藏",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "shopJoin/shopJoin",
          "style": {
            "navigationBarTitleText": "商家入驻",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "expertJoin/expertJoin",
          "style": {
            "navigationBarTitleText": "达人入驻",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "coupon/coupon",
          "style": {
            "navigationBarTitleText": "优惠券",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "order/order",
          "style": {
            "navigationBarTitleText": "订单",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "哈米圈圈",
    "navigationBarBackgroundColor": "#ffffff",
    "backgroundColor": "#F8F8F8"
  },
  // 设置 TabBar
  "tabBar": {
    "color": "#9499a0",
    "selectedColor": "#fb5383",
    "backgroundColor": "#fff",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/home/home",
        "iconPath": "static/tabs/home.png",
        "selectedIconPath": "static/tabs/home_select.png"
      },
      {
        "text": "发现",
        "pagePath": "pages/find/find",
        "iconPath": "static/tabs/find.png",
        "selectedIconPath": "static/tabs/find_select.png"
      },
      {
        "text": "Ai智能体",
        "pagePath": "pages/ai/ai",
        "iconPath": "static/tabs/ai.png",
        "selectedIconPath": "static/tabs/ai_select.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/my",
        "iconPath": "static/tabs/my.png",
        "selectedIconPath": "static/tabs/my_select.png"
      }
    ]
  }
}
